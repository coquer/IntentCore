<intents>
	<intent>
		<name>PickFile</name>
		<action android:name="org.openintents.action.PICK_FILE" />
		<data android:mimeType="" />
		<data android:Uri="file:///path/to/your/file.txt" />
		<category android:name="" />
		<exceptionName></exceptionName>
		<sucessCallback></sucessCallback>
		<extras>
			<extra>
				<type>StringExtra</type>
				<name>org.openintents.extra.TITLE</name>
				<value>Please select a value</value>
			</extra>
		</extras>
	</intent>
	<intent>
		<name>FlashLight</name>
		<action android:name="com.telsacoilsw.intent.FLASHLIGHT" />
		<data android:mimeType="" />
		<data android:Uri="" />
		<category android:name="" />
		<exceptionName></exceptionName>
		<sucessCallback></sucessCallback>
		<extras>
			<extra>
				<type>BooleanExtra</type>
				<name>on</name>
				<value>false</value>
			</extra>
			<extra>
				<type>IntExtra</type>
				<name>timeout</name>
				<value>0</value>
			</extra>
			<extra>
				<type>BooleanExtra</type>
				<name>off</name>
				<value>true</value>
			</extra>
			<extra>
				<type>BooleanExtra</type>
				<name>toggle</name>
				<value>false</value>
			</extra>
			<extra>
				<type>IntExtra</type>
				<name>strobe</name>
				<value>0</value>
			</extra>
		</extras>
	</intent>
	<intent>
		<name>ActionWebSearch</name>
		<action android:name="android.intent.action.WEB_SEARCH" />
		<data android:mimeType="" />
		<data android:Uri="" />
		<category android:name="android.intent.category.DEFAULT" />
		<exceptionName></exceptionName>
		<sucessCallback></sucessCallback>
		<extras>
			<extra>
				<type>StringExtra</type>
				<name>query</name>
				<value>Insert query or URL</value>
			</extra>
		</extras>
	</intent>
	<intent>
		<name>ActionCall</name>
		<action android:name="android.intent.action.CALL" />
		<data android:mimeType="" />
		<data android:Uri="tel:555-555-5555" />
		<category android:name="android.intent.category.DEFAULT" />
		<exceptionName></exceptionName>
		<sucessCallback></sucessCallback>
		<extras>
		</extras>
	</intent>
	
	<intent>
		<name>ActionDial</name>
		<action android:name="android.intent.action.DIAL" />
	</intent>
	<intent>
		<name>ActionSendText</name>
		<action android:name="android.intent.action.SEND" />
		<data android:mimeType="text/plain" />
		<data android:Uri="" />
		<category android:name="android.intent.category.DEFAULT" />
		<exceptionName></exceptionName>
		<sucessCallback></sucessCallback>
		<extras>
			<extra>
				<type>StringExtra</type>
				<name>android.intent.extra.TEXT</name>
				<value>Put your text here</value>
			</extra>
		</extras>
	</intent>
	<intent>
		<name>ActionSendStream</name>
		<action android:name="android.intent.action.SEND" />
		<data android:mimeType="image/jpeg" />
		<data android:Uri="" />
		<category android:name="android.intent.category.DEFAULT" />
		<exceptionName></exceptionName>
		<sucessCallback></sucessCallback>
		<extras>
		<extra>
			<type>UriExtra</type>
			<name>android.intent.extra.STREAM</name>
			<value>file:///path/to/your/file</value>
		</extra>
		</extras>
	</intent>
</intents>
